{
  "commands": {
    "commandreference:generate": {
      "aliases": [],
      "args": {},
      "description": "generate the command reference guide located",
      "examples": [
        "Generate the command reference guide\n<%= config.bin %> <%= command.id %>",
        "Generate the command reference for a set of plugins\n<%= config.bin %> <%= command.id %> --plugins pluginA,pluginB",
        "Generate the command reference for all plugins\n<%= config.bin %> <%= command.id %> --all --output-dir ./docs",
        "Generate the command reference for all plugins in a directory\n<%= config.bin %> <%= command.id %> --all --config-path /path/to/plugin --output-dir ./docs"
      ],
      "flags": {
        "json": {
          "description": "Format output as json.",
          "helpGroup": "GLOBAL",
          "name": "json",
          "allowNo": false,
          "type": "boolean"
        },
        "flags-dir": {
          "helpGroup": "GLOBAL",
          "name": "flags-dir",
          "summary": "Import flag values from a directory.",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "output-dir": {
          "aliases": [
            "outputdir"
          ],
          "char": "d",
          "deprecateAliases": true,
          "name": "output-dir",
          "summary": "output directory to put generated files",
          "default": "./tmp/root",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "plugins": {
          "char": "p",
          "exclusive": [
            "all"
          ],
          "name": "plugins",
          "summary": "comma separated list of plugin names to be part of the generation. Defaults to the oclif plugin in the current working directory",
          "hasDynamicHelp": false,
          "multiple": true,
          "type": "option"
        },
        "all": {
          "char": "a",
          "exclusive": [
            "plugins"
          ],
          "name": "all",
          "summary": "include all relevant plugins in the generation",
          "allowNo": false,
          "type": "boolean"
        },
        "ditamap-suffix": {
          "char": "s",
          "name": "ditamap-suffix",
          "summary": "unique suffix to append to generated ditamap",
          "default": "unified",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "hidden": {
          "name": "hidden",
          "summary": "show hidden commands",
          "allowNo": false,
          "type": "boolean"
        },
        "error-on-warnings": {
          "aliases": [
            "erroronwarnings"
          ],
          "deprecateAliases": true,
          "name": "error-on-warnings",
          "summary": "fail the command if there are any warnings",
          "allowNo": false,
          "type": "boolean"
        },
        "config-path": {
          "char": "c",
          "name": "config-path",
          "summary": "A path to the directory containing a plugin or cli",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "commandreference:generate",
      "pluginAlias": "@salesforce/plugin-command-reference",
      "pluginName": "@salesforce/plugin-command-reference",
      "pluginType": "core",
      "strict": true,
      "summary": "generate the command reference guide located",
      "enableJsonFlag": true,
      "isESM": true,
      "relativePath": [
        "lib",
        "commands",
        "commandreference",
        "generate.js"
      ],
      "aliasPermutations": [],
      "permutations": [
        "commandreference:generate",
        "generate:commandreference"
      ]
    },
    "jit:install": {
      "aliases": [],
      "args": {},
      "examples": [
        "<%= config.bin %> <%= command.id %>"
      ],
      "flags": {
        "json": {
          "description": "Format output as json.",
          "helpGroup": "GLOBAL",
          "name": "json",
          "allowNo": false,
          "type": "boolean"
        },
        "flags-dir": {
          "helpGroup": "GLOBAL",
          "name": "flags-dir",
          "summary": "Import flag values from a directory.",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "dry-run": {
          "char": "d",
          "name": "dry-run",
          "summary": "List the plugins that would be installed.",
          "allowNo": false,
          "type": "boolean"
        }
      },
      "hasDynamicHelp": false,
      "hiddenAliases": [],
      "id": "jit:install",
      "pluginAlias": "@salesforce/plugin-command-reference",
      "pluginName": "@salesforce/plugin-command-reference",
      "pluginType": "core",
      "strict": true,
      "summary": "Install all JIT plugins.",
      "enableJsonFlag": true,
      "isESM": true,
      "relativePath": [
        "lib",
        "commands",
        "jit",
        "install.js"
      ],
      "aliasPermutations": [],
      "permutations": [
        "jit:install",
        "install:jit"
      ]
    }
  },
  "version": "3.1.8"
}